{
	"expo": {
		"name": "crah",
		"slug": "crah",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "crah",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.crahteam.crah",
			"permissions": ["android.permission.CAMERA"]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-secure-store",
			"expo-video",
			"react-native-vision-camera"
		],
		"experiments": {
			"typedRoutes": true,
			"react-native-fabric": false
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "5531724a-25d6-4787-8491-a3365af718d5"
			}
		},
		"owner": "crahteam"
	}
}
