{
  "expo": {
    "name": "crah-app",
    "slug": "crah-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/crah.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/crah.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff0000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/crah.png",
        "backgroundColor": "#ff0000"
      },
      "package": "com.anonymous.crahapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/crah.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "expoRouterAppRoot": "/"
    }
  }
}
