{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "types/expo-env.d.ts",
    "index.js",
    "app/**/*",
    "types/**/*",
    "../crah-mobile-app/types/react-native-image-crop-picker.d.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
